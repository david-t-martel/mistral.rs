{
  "servers": [
    {
      "name": "Memory",
      "source": {
        "type": "Process",
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-memory@2025.8.4"],
        "env": {
          "MEMORY_FILE_PATH": "T:/projects/rust-mistral/mistral.rs/memory.json",
          "MCP_PROTOCOL_VERSION": "2025-06-18"
        }
      },
      "security_policy": {
        "id": "memory-moderate",
        "description": "Moderate policy for memory server - file access only to memory.json",
        "filesystem": {
          "allowed_paths": ["T:/projects/rust-mistral/mistral.rs"],
          "blocked_paths": [],
          "allowed_extensions": [".json"],
          "blocked_extensions": [".exe", ".dll", ".so", ".ps1", ".sh"],
          "max_file_size": 10485760,
          "allow_hidden": false,
          "allow_symlinks": false,
          "allow_write": true,
          "allow_delete": false
        },
        "process": {
          "allowed_commands": [],
          "blocked_commands": ["*"],
          "allowed_args_patterns": [],
          "blocked_args_patterns": [],
          "max_args": 0,
          "max_arg_length": 0,
          "allow_shell": false
        },
        "network": {
          "allowed_urls": [],
          "blocked_urls": ["*"],
          "allowed_protocols": [],
          "allowed_ports": null,
          "block_private_ips": true,
          "block_loopback": true
        },
        "environment": {
          "allowed_vars": ["PATH", "HOME", "USER", "MEMORY_FILE_PATH", "MCP_PROTOCOL_VERSION"],
          "blocked_vars": ["LD_PRELOAD", "LD_LIBRARY_PATH", "DYLD_INSERT_LIBRARIES"],
          "sanitize_vars": [],
          "allow_passthrough": false
        },
        "rate_limits": {
          "max_requests_per_minute": 60,
          "max_concurrent": 5,
          "max_total_operations": 1000
        },
        "audit": {
          "log_all_operations": false,
          "log_failures": true,
          "log_sensitive_access": true,
          "include_arguments": false
        },
        "strict_mode": true
      }
    },
