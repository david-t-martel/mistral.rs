{
  "servers": [
    {
      "name": "Memory",
      "source": {
        "type": "Process",
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-memory@2025.8.4"],
        "env": {
          "MEMORY_FILE_PATH": "T:/projects/rust-mistral/mistral.rs/memory.json",
          "MCP_PROTOCOL_VERSION": "2025-06-18"
        }
      }
    },
    {
      "name": "Filesystem",
      "source": {
        "type": "Process",
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-filesystem@2025.8.21", "T:/projects/rust-mistral/mistral.rs"],
        "env": {
          "MCP_PROTOCOL_VERSION": "2025-06-18"
        }
      }
    },
    {
      "name": "Sequential Thinking",
      "source": {
        "type": "Process",
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-sequential-thinking@2025.7.1"],
        "env": {
          "MCP_PROTOCOL_VERSION": "2025-06-18"
        }
      }
    },
    {
      "name": "GitHub",
      "source": {
        "type": "Process",
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-github@2025.4.8"],
        "env": {
          "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}",
          "MCP_PROTOCOL_VERSION": "2025-06-18"
        }
      }
    },
    {
      "name": "Fetch",
      "source": {
        "type": "Process",
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-fetch@0.6.3"],
        "env": {
          "MCP_PROTOCOL_VERSION": "2025-06-18"
        }
      }
    },
    {
      "name": "Time",
      "source": {
        "type": "Process",
        "command": "npx",
        "args": ["-y", "@theo.foobar/mcp-time"],
        "env": {
          "MCP_PROTOCOL_VERSION": "2025-06-18"
        }
      }
    },
    {
      "name": "Serena Claude",
      "source": {
        "type": "Process",
        "command": "uv",
        "args": ["run", "python", "T:/projects/mcp_servers/serena/scripts/mcp_server.py"],
        "env": {
          "MCP_PROTOCOL_VERSION": "2025-06-18",
          "PYTHONUNBUFFERED": "1"
        }
      }
    },
    {
      "name": "Python FileOps Enhanced",
      "source": {
        "type": "Process",
        "command": "uv",
        "args": ["--directory", "C:/Users/david/.claude/python_fileops", "run", "python", "-m", "desktop_commander.mcp_server"],
        "work_dir": "C:/Users/david/.claude/python_fileops",
        "env": {
          "PYTHONUNBUFFERED": "1",
          "LOG_LEVEL": "ERROR",
          "RUST_LOG": "error",
          "PYTHON_FILEOPS_MAX_SESSIONS": "10",
          "PYTHON_FILEOPS_SESSION_TIMEOUT": "3600",
          "PYTHON_FILEOPS_ENABLE_RUST": "true",
          "PYTHON_FILEOPS_GITIGNORE_AWARE": "true",
          "PYTHON_FILEOPS_ZERO_COPY": "true",
          "PYTHON_FILEOPS_WORKER_POOL": "true",
          "PYTHON_FILEOPS_SIMD_ENABLED": "true",
          "PYTHON_FILEOPS_MEMORY_MAPPED": "true",
          "PYTHON_FILEOPS_ASYNC_ENABLED": "true",
          "PYTHON_FILEOPS_SECURITY_ENABLED": "true",
          "PYTHON_FILEOPS_THREAD_POOL_SIZE": "32",
          "PYTHON_FILEOPS_BUFFER_POOL_ENABLED": "true",
          "PYTHON_FILEOPS_PERFORMANCE_MONITORING": "true",
          "MCP_PROTOCOL_VERSION": "2025-06-18",
          "DESKTOP_COMMANDER_SECURITY": "true",
          "DESKTOP_COMMANDER_RATE_LIMIT": "60",
          "DESKTOP_COMMANDER_AUDIT": "true"
        }
      }
    },
    {
      "name": "RAG Redis",
      "source": {
        "type": "Process",
        "command": "C:/users/david/bin/rag-redis-mcp-server.exe",
        "args": [],
        "env": {
          "REDIS_URL": "redis://127.0.0.1:6379",
          "RUST_LOG": "info",
          "RUST_BACKTRACE": "1",
          "RAG_DATA_DIR": "C:/codedev/llm/rag-redis/data/rag",
          "EMBEDDING_CACHE_DIR": "C:/codedev/llm/rag-redis/cache/embeddings",
          "LOG_DIR": "C:/codedev/llm/rag-redis/logs",
          "LOG_LEVEL": "INFO"
        }
      }
    }
  ],
  "auto_register_tools": true,
  "tool_timeout_secs": 180,
  "max_concurrent_calls": 3
}
